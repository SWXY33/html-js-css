/* pages/phonelogin/phonelogin.wxss *//* pages/login2/login2.wxss */.header {
 margin: 90rpx 0 90rpx 50rpx;
 border-bottom: 1px solid #ccc;
 text-align: center;
 width: 650rpx;
 height: 300rpx;
 line-height: 450rpx;
}
 
.userHead {
  width: 70rpx;
  height: 70rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userTitle {
  margin-left: 16rpx;
  font-size: 30rpx;
  color: #333;
}
.header image {
 width: 200rpx;
 height: 200rpx;
}
 
.content {
 margin-left: 50rpx;
 margin-bottom: 90rpx;
}
 
.content text {
 display: block;
 color: #9d9d9d;
 margin-top: 40rpx;
}
 
.bottom {
 border-radius: 80rpx;
 margin: 70rpx 50rpx;
 font-size: 35rpx;
}
/* pages/mine/mine.wxss */
.head{
  height: 170rpx;
  line-height: 170rpx;
  border-bottom: 1rpx solid #ccc;
}
.head text{
  margin-left: 30rpx;
}
.head image{
  margin: 30rpx;
  width: 100rpx;
  height: 100rpx;
  float: right;
  border-radius: 5rpx;
}
.name{
  line-height: 170rpx;
  float: right;
  margin-right: 30rpx;
}
.phone{
  line-height: 170rpx;
  float: right;
  margin-right: 30rpx;
}
.changeUser{
  height: 150rpx;
  border-top: 130rpx solid rgb(230, 228, 228);
  text-align: center;
  border-bottom: 1rpx solid #ccc;
  line-height: 150rpx;
}